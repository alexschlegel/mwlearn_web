  add: (name, fTest) -> @_tests[name] = fTest

  run: (name) ->
    fTest = =>
      try
        @_tests[name](@_mwl)
        alert "#{name} succeeded!"
      catch error
        alert "#{name} failed (#{error})!"

    @_mwl.queue.add name, fTest

  runAll: -> @run(name) for name of @_tests
